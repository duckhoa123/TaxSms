    <select id="selectByExampleNewKPI" resultMap="BaseResultMap">
        select a.* from sys_log a
        where
        (LOWER(a.user_name) like ('%' || LOWER(#{filterText}) || '%'))
        and a.user_name = #{userName}
        order by a.create_time desc
    </select>

    <select id="selectByExampleNewC2C" resultMap="BaseResultMap">
        select a.* from sys_log@profilec2c a
        where
        (LOWER(a.user_name) like ('%' || LOWER(#{filterText}) || '%'))
        and a.user_name = #{userName}
        order by a.create_time desc
    </select>
